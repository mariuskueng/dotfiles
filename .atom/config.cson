"*":
  "release-notes":
    viewedVersion: "0.95.0"
  welcome:
    showOnStartup: false
  editor:
    fontSize: 12
    showIndentGuide: true
    useShadowDOM: true
    invisibles: {}
    fontFamily: "Menlo"
  core:
    disabledPackages: [
      "metrics"
      "feedback"
      "atom-lint"
      "exception-reporting"
    ]
    themes: [
      "spacegray-dark-ui"
      "base16-ocean-dark-spacegray"
    ]
  "linter-flake8":
    ignoreErrorCodes: [
      "E501"
    ]
  linter:
    subtleLinterErrors: [
      "scss-lint"
    ]
  minimap: {}
  "atom-lint": {}
  tabs:
    showIcons: true
  "tree-view":
    hideVcsIgnoredFiles: false
    hideIgnoredNames: false
    showOnRightSide: false
  "autocomplete-plus": {}
  "linter-scss-lint":
    scssLintExecutablePath: "/Library/Ruby/Gems/2.0.0/gems/scss-lint-0.38.0/bin/scss-lint"
